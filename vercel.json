{
    "version": 2,
    "builds": [
      {
        "src": "TextSum/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "TextSum/wsgi.py"
      }
    ]
  }
  
